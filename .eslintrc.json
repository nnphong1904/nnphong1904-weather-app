{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": [
		"plugin:react/recommended",
		"google",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"plugins": ["react", "prettier"],
	"rules": {
		"prettier/prettier": [
			"error",
			{ "singleQuote": true }
		],
		"require-jsdoc": "off",
		"no-unused-vars": "warn",
		"react/prop-types": [0]
	}
}
